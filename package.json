{
	"name": "@titanium/facebook",
	"version": "0.0.2",
	"description": "Native modules that allows you to use Facebook SDK with Axway Titanium native mobile apps.",
	"main": "index.js",
	"scripts": {
		"list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "tiapp-remove-module facebook && tiapp-add-module facebook ios 6.1.0 && tiapp-add-module facebook android 8.1.0",
		"postuninstall": "tiapp-remove-module facebook",
		"test": "npm install --ignore-scripts && npm run list-files"
	},
	"author": {
		"name": "Brenton House",
		"email": "brenton.house@gmail.com",
		"url": "https://brenton.house"
	},
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-facebook"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-facebook/issues"
	},
	"homepage": "https://github.com/brentonhouse/titanium-facebook#readme",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"facebook",
		"titanium-native-module",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": [
			"ios",
			"android"
		],
		"native": [
			{
				"name": "facebook",
				"platform": "ios",
				"version": "6.1.0"
			},
			{
				"name": "ti.animation",
				"platform": "android",
				"version": "8.1.0"
			}
		]
	},
	"dependencies": {
		"@titanium/tiapp-xml": "^0.4.2"
	}
}
